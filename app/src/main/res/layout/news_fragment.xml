<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".News_Fragment"
    tools:ignore="ScrollViewCount"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/hindustatimes"
                android:layout_width="220dp"
                android:layout_height="153dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="8dp"
                android:clickable="true"
                android:contentDescription="TODO"
                android:focusable="true"
                android:src="@drawable/hindustan"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.052"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.021"
                tools:ignore="ContentDescription,HardcodedText" />

            <ImageButton
                android:id="@+id/midday"
                android:layout_width="wrap_content"
                android:layout_height="161dp"
                android:layout_marginStart="-19.5dp"
                android:layout_marginEnd="9dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/midday"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.054"
                tools:ignore="ContentDescription,HardcodedText" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/times"
                android:layout_width="163dp"
                android:layout_height="140dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="204dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/toi"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.25"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.451"
                tools:ignore="ContentDescription,HardcodedText" />

            <ImageButton
                android:id="@+id/raftar"
                android:layout_width="174dp"
                android:layout_height="143dp"
                android:layout_marginStart="-159.5dp"
                android:layout_marginEnd="4dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/raftaar"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.831"
                tools:ignore="ContentDescription,HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/lokmats"
                android:layout_width="193dp"
                android:layout_height="147dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/lokmats"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.954"
                app:layout_constraintStart_toStartOf="@+id/raftar"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.433"
                tools:ignore="ContentDescription,HardcodedText" />

            <ImageButton
                android:id="@+id/ndtvs"
                android:layout_width="166dp"
                android:layout_height="132dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/ndtv"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/lokmats"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.845"
                tools:ignore="ContentDescription,HardcodedText" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/nhps"
                android:layout_width="179dp"
                android:layout_height="140dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="204dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/nhp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.25"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.451"
                tools:ignore="ContentDescription,HardcodedText" />

            <ImageButton
                android:id="@+id/healthlines"
                android:layout_width="174dp"
                android:layout_height="143dp"
                android:layout_marginStart="-159.5dp"
                android:layout_marginEnd="4dp"
                android:clickable="true"
                android:focusable="true"
                android:src="@drawable/healthline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.831"
                tools:ignore="ContentDescription,HardcodedText" />
        </LinearLayout>
    </LinearLayout>

</ScrollView>